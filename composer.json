{
	"require": {
		"laravel/framework": "5.0.*",
		"illuminate/html": "5.0.*",
		"shnhrrsn/laravel-assets": "dev-master",
		"filp/whoops": "1.1.*"
	},
	"repositories": [
		{
			"type": "git",
			"url" : "http://git2.enormego.com/shnhrrsn/laravel-assets.git"
		}
	],
	"require-dev": {
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
